{
    "manifest_version": 2,
    "name": "RDAP Lookup Utility",
    "version": "1.3.0",
    "description": "Display more informations of a domain/ipv4/ipv6/nameserver",
    "author": "ISIF-UM",
    "background": {
        "scripts": ["background.js", "js/psl.min.js"],
        "persistent": false
    },
    "browser_action":{
        "default_popup": "index.html",
        "default_icon": {
            "128": "rdap-128.png"
          }  
    },
    "permissions": [
        "tabs"
        // "tabs", "background" this one from chrome extension 
    ],
    "icons":{
        "128": "rdap-128.png"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}